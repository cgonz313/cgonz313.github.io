	#outlook a { 
		padding:0; 
	}

	table{
	    mso-table-lspace:0pt;
	    mso-table-rspace:0pt;
	}

	body { 
		width:100% !important; 
		height:100% !important;
		-webkit-text-size-adjust:100%; 
		-ms-text-size-adjust:none; 
		margin:0;
		padding:0; 
	} 

	img{
        -ms-interpolation-mode:bicubic;
        border: 0;
        outline: none;
        text-decoration: none;
    }
    .imageFix {
    	display: block;
    }

	.ReadMsgBody { 
		width: 100%; 
	}

	.ExternalClass {
		width:100%;
	}

	.ExternalClass,
	.ExternalClass p,
	.ExternalClass span,
	.ExternalClass font,
	.ExternalClass td,
	.ExternalClass div{
	    line-height: 100%;
	}

	div, p, a, li, td { 
		-webkit-text-size-adjust:none; 
	}

	table td {
		border-collapse: collapse;
	}
 
	

	*[class="container"] {
