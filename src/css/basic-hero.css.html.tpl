	#outlook a { 
		padding:0; 
	}
	body { 
		width:100% !important; 
		-webkit-text; size-adjust:100%; 
		-ms-text-size-adjust:100%; 
		margin:0;
		padding:0; 
	}     
	.ReadMsgBody { 
		width: 100%; 
	}
	.ExternalClass {
		width:100%;
	} 
	table td {
		border-collapse: collapse;
	}
	.ExternalClass * {
		line-height: 115%;
	}   

	a.privacy-tos {
		color: #ffffff !important;
